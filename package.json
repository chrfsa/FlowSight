{
  "name": "flowsight",
  "publisher": "local",
  "displayName": "FlowSight",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flowsight.open",
        "title": "FlowSight: Open Monitor"
      }
    ],
    "configuration": {
      "title": "FlowSight Tracing",
      "properties": {
        "flowsight.tracing.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable automatic LangChain tracing to the FlowSight local server (Port 1984)."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^18.19.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/body-parser": "^1.19.5",
    "typescript": "^5.3.3"
  }
}